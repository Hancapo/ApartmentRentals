<Page
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DepartamentoApp"
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes" 
      xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
      x:Class="DepartamentoApp.InicioPage"
      mc:Ignorable="d" 
      d:DesignHeight="680" d:DesignWidth="1160"
      Title="InicioPage">

    <Grid>

        <lvc:CartesianChart Foreground="{StaticResource MaterialDesignDarkForeground}"  x:Name="ChartRegionesDep" Width="1000" Height="250" LegendLocation="Bottom">
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Foreground="{StaticResource MaterialDesignDarkForeground}" x:Name="AxisRegionsDep" Title="Cantidad de Departamentos" FontFamily="{StaticResource MaterialDesignFont}" FontSize="10"/>
            </lvc:CartesianChart.AxisX>
        </lvc:CartesianChart>
    </Grid>
</Page> 
